{
  "name": "recipe-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel serve server",
    "tpublish": "npm run gitRemoteSetPublish && npm run tbuild && npm run tdeploy && npm run gitRemoteRestore",
    "serve": "ng serve -o",
    "server": "json-server --watch edamam.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gitRemoteSetPublish": "git remote set-url origin https://github.com/daniel-salin/u06-production.git",
    "gitRemoteRestore": "git remote set-url origin https://github.com/chas-academy/06-recipe-app-frontend-daniel-salin.git",
    "tbuild": "ng build --prod --base-href 'http://recipe-app.danielsalin.chas.academy/'",
    "tdeploy": "ngh --dir dist/recipe-app"
    
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.6.4",
    "jquery": "^3.3.1",
    "json-server": "^0.14.2",
    "materialize-css": "^1.0.0-rc.2",
    "popper": "^1.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.21",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typescript": "3.1.6"
  }
}
